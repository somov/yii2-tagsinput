{
    "name": "somov/yii2-tagsinput",
    "description": "The yii2-tagsinput is a Yii 2 wrapper for [bootstrap-tagsinput]()",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "extension",
        "yii2-tagsinput",
    ],
    "homepage": "https://github.com/somov/yii2-tagsinput",
    "license": "BSD-3-Clause",
    "require": {
        "yiisoft/yii2": "~2.0",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "bower-asset/typeahead.js": "0.10.* | ~0.11.0",
        "life2016/bootstrap-tagsinput": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "somov\\tagsinput\\": "src"
        }
    }
}